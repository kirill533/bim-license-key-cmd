{
  "name": "bim/license-key-cmd",
  "description": "Library for validation license key through command line interface (cmd).",
  "version": "1.0.0",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Kyrylo Kostiukov",
      "homepage": "https://www.bimprojec.net/"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nategood/commando": "*",
    "wp-cli/php-cli-tools": "0.11.9",
    "wikimedia/composer-merge-plugin": "2.0.*",
    "bim/license-key": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "CmdLicenseKey\\": "src/"
    }
  },
  "bin": ["bin/bim_license"],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kirill533/bim-license-key"
    }
  ],
  "config": {
    "allow-plugins": {
      "wikimedia/composer-merge-plugin": true
    }
  }
}
